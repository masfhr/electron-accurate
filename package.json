{
  "name": "accurate-import-envision-x",
  "version": "3.7.9",
  "description": "Accurate Online Import Database Envision application",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.masfhr.accurate-import-envision-x",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "shorticon.ico",
      "uninstallerIcon": "shorticon.ico",
      "uninstallDisplayName": "accurate-import-envision-x-uninstaller",
      "license": "LICENSE.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "masfhr",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8"
    
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "axios": "^1.3.4",
    "jquery": "^3.6.4",
    "nedb": "^1.8.0"
  }
}
